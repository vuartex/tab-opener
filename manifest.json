{
  "manifest_version": 3,
  "name": "Tab Opener",
  "version": "1.2",
  "description": "Kullanıcının belirlediği URL'leri yeni sekmelerde açar.",
  "permissions": ["storage", "tabs", "commands"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "options_page": "options.html"
}